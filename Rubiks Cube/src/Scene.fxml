<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="811.0" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller">
   <HBox>
      <VBox fx:id="controlPane" maxWidth="211.0" minWidth="211.0" prefHeight="500.0" prefWidth="210.0">
         <HBox prefHeight="40.0" prefWidth="200.0">
            <ToggleButton fx:id="acToggle" mnemonicParsing="false" prefHeight="40.0" text="AntiClockwise">
               <HBox.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </HBox.margin>
            </ToggleButton>
            <ToggleButton fx:id="wideToggle" mnemonicParsing="false" prefHeight="40.0" text="Wide">
               <HBox.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </HBox.margin>
            </ToggleButton>
         </HBox>
         <GridPane prefHeight="160.0">
            <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
               <RowConstraints/>
               <RowConstraints/>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <VBox.margin>
               <Insets top="4.0"/>
            </VBox.margin>
            <Button fx:id="uButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="U" textAlignment="CENTER" GridPane.columnIndex="1"
                    GridPane.rowIndex="2">
               <GridPane.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </GridPane.margin>
            </Button>
            <Button fx:id="rButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="R" textAlignment="CENTER" GridPane.columnIndex="2"
                    GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </GridPane.margin>
            </Button>
            <Button fx:id="lButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="L" textAlignment="CENTER" GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </GridPane.margin>
            </Button>
            <Button fx:id="bButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="B" textAlignment="CENTER" GridPane.columnIndex="1"
                    GridPane.rowIndex="5">
               <GridPane.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </GridPane.margin>
            </Button>
            <Button fx:id="dButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="D" textAlignment="CENTER" GridPane.columnIndex="1"
                    GridPane.rowIndex="4">
               <GridPane.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </GridPane.margin>
            </Button>
            <Button fx:id="fButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="F" textAlignment="CENTER" GridPane.columnIndex="1"
                    GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </GridPane.margin>
            </Button>
         </GridPane>
         <HBox prefHeight="40.0" prefWidth="200.0">
            <VBox.margin>
               <Insets top="5.0"/>
            </VBox.margin>
            <Button fx:id="mButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="M" textAlignment="CENTER">
               <HBox.margin>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
               </HBox.margin>
            </Button>
            <Button fx:id="eButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="E" textAlignment="CENTER">
               <HBox.margin>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
               </HBox.margin>
            </Button>
            <Button fx:id="sButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="S" textAlignment="CENTER">
               <HBox.margin>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
               </HBox.margin>
            </Button>
         </HBox>
         <HBox prefWidth="200.0">
            <VBox.margin>
               <Insets bottom="10.0" top="2.0"/>
            </VBox.margin>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0"
                    prefWidth="67.0" text="x" textAlignment="CENTER" fx:id="xButton">
               <HBox.margin>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
               </HBox.margin>
            </Button>
            <Button fx:id="yButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="y" textAlignment="CENTER">
               <HBox.margin>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
               </HBox.margin>
            </Button>
            <Button fx:id="zButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                    prefHeight="40.0" prefWidth="67.0" text="z" textAlignment="CENTER">
               <HBox.margin>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
               </HBox.margin>
            </Button>
         </HBox>
         <HBox prefHeight="40.0" prefWidth="230.0">
            <Button fx:id="shuffleButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="67.0"
                    text="Shuffle!">
               <HBox.margin>
                  <Insets left="1.0" right="1.0"/>
               </HBox.margin>
            </Button>
            <Button fx:id="resetButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="67.0"
                    text="Reset">
               <HBox.margin>
                  <Insets left="1.0" right="1.0"/>
               </HBox.margin>
            </Button>
         </HBox>
         <Text fx:id="shuffleText" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="210.0">
            <VBox.margin>
               <Insets bottom="5.0" top="5.0"/>
            </VBox.margin>
         </Text>
         <TextField fx:id="algTextBox" promptText="Enter some instructions...">
            <VBox.margin>
               <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
            </VBox.margin>
         </TextField>
         <HBox prefHeight="40.0" prefWidth="211.0">
            <Button fx:id="algButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="123.0"
                    text="Do an algorithm!">
               <HBox.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </HBox.margin>
            </Button>
            <Button fx:id="clearButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="67.0"
                    text="Clear">
               <HBox.margin>
                  <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
               </HBox.margin>
            </Button>
         </HBox>
         <Button fx:id="camResetButton" mnemonicParsing="false" prefHeight="40.0" text="Reset Camera">
            <VBox.margin>
               <Insets bottom="2.0" left="1.0" right="1.0" top="2.0"/>
            </VBox.margin>
         </Button>
      </VBox>
      <Pane fx:id="viewPane" prefHeight="600.0" prefWidth="600.0"/>
   </HBox>
</AnchorPane>
